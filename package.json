    ],
    "*.css": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "dayjs": "^1.11.11",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "recoil": "^0.7.7",
    "strip-ansi": "^7.1.0",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "postcss": "^8",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
